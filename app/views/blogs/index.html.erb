      <% @blogs.each do |blog| %>
      <div class="card white darken-1">
        <div class="card-content">
          <span class="card-title"><%= blog.title %></span>
          <p><%= blog.text %></p>
        </div>
        <div class="card-action">
          <a href="/blogs/<%= blog.id %>/edit">編集</a>
          <a href="/blogs/<%= blog.id %>" data-method="delete">削除</a>

        </div>
      </div>
      <% end %>
